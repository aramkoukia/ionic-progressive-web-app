{
  "name": "@stencil/starter",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Starter App",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "stencil build --stats",
    "dev": "stencil build --dev --watch --serve",
    "devWithSW": "sd concurrent \"stencil build --dev --service-worker --watch\"",
    "functionDev": "stencil build --dev --watch",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "deploy": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@ionic/core": "^4.0.0-beta.0",
    "@stencil/core": "^0.10.9",
    "am-rating": "0.0.7",
    "firebase": "4.8.2",
    "idb-keyval": "^3.0.5",
    "st-img": "^0.3.0"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/googlemaps": "^3.30.10"
  },
  "repository": {
    "type": "git",
